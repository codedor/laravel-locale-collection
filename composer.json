{
    "name": "wotz/laravel-locale-collection",
    "description": "Locale Collection package for Laravel",
    "keywords": [
        "who owns the zebra",
        "laravel",
        "laravel-locale-collection"
    ],
    "homepage": "https://github.com/wotzebra/laravel-locale-collection",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.4 || ^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "illuminate/cookie": "^10.4 || ^11.0 || ^12.0",
        "illuminate/support": "^10.4 || ^11.0 || ^12.0",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0 || ^8.0",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\LocaleCollection\\": "src",
            "Wotz\\LocaleCollection\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\LocaleCollection\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wotz\\LocaleCollection\\Providers\\LocaleCollectionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
