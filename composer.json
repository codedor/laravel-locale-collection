{
    "name": "codedor/laravel-locale-collection",
    "description": "Locale Collection package for Laravel",
    "keywords": [
        "codedor",
        "laravel",
        "laravel-locale-collection"
    ],
    "homepage": "https://github.com/codedor/laravel-locale-collection",
    "license": "MIT",
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/contracts": "^10.4|^11.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "illuminate/cookie": "^10.4|^11.0",
        "illuminate/support": "^10.4|^11.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\LocaleCollection\\": "src",
            "Codedor\\LocaleCollection\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\LocaleCollection\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\LocaleCollection\\Providers\\LocaleCollectionServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-feature/laravel-11": "1.1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
